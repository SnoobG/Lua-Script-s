_G.Owner = function(Name)

validPrefix = '.'
local Players = game:GetService("Players")
local cp = game.Players.LocalPlayer.PlayerGui:FindFirstChild("HDAdminGUIs").MainFrame.Pages.Settings.Custom
    ["AE1 Prefix"].SettingValue.TextBox.Text
local function chat(msg)
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
end
vers = "v0.1"

local fuckdan = game:GetService("Players")

local function getPlr(Name)
    if Name:lower() == "random" then
        return fuckdan:GetPlayers()[math.random(#fuckdan:GetPlayers())]
    else
        local cleanName = Name:lower():gsub("%s", "")
        for _, x in pairs(fuckdan:GetPlayers()) do
            local playerName = x.Name:lower():gsub("%s", "")
            local displayName = x.DisplayName:lower():gsub("%s", "")

            if playerName:match(cleanName) or displayName:match("^" .. cleanName) then
                return x
            end
        end
    end
end
local RunService = game:GetService("RunService")
plr = game.Players.LocalPlayer
local char = plr.Character
Char = char
speaker = plr
ReplicatedStorage = game:GetService("ReplicatedStorage")
spinSpeed = 20
-- تحديد معلومات اللاعب

function r15(plr)
    if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').RigType == Enum.HumanoidRigType.R15 then
        return true
    end
end

function getTorso(x)
    x = x or game.Players.LocalPlayer.Character
    return x:FindFirstChild("Torso") or x:FindFirstChild("UpperTorso") or x:FindFirstChild("LowerTorso") or
        x:FindFirstChild("HumanoidRootPart")
end

function getRoot(char)
    local rootPart = game.Players.LocalPlayer.Character:FindFirstChild('HumanoidRootPart') or
        game.Players.LocalPlayer.Character:FindFirstChild('Torso') or
        game.Players.LocalPlayer.Character:FindFirstChild('UpperTorso')
    return rootPart
end

function emote()
    local Client = game.Players.LocalPlayer
    Client["Character"]:FindFirstChild("Animate").Disabled = false
    for i, v in pairs(Client.Character.Humanoid:GetPlayingAnimationTracks()) do v:Stop() end
    Client.Character.Animate.idle.Animation1.AnimationId = "rbxassetid://" .. 10921259953
    Client.Character.Animate.idle.Animation2.AnimationId = "rbxassetid://" .. 10921258489
    Client.Character.Animate.walk.WalkAnim.AnimationId = "rbxassetid://" .. 10921269718
    Client.Character.Animate.run.RunAnim.AnimationId = "rbxassetid://" .. 10921261968
    Client.Character.Animate.jump.JumpAnim.AnimationId = "rbxassetid://" .. 10921263860
    Client.Character.Animate.climb.ClimbAnim.AnimationId = "rbxassetid://" .. 10921257536
    Client.Character.Animate.fall.FallAnim.AnimationId = "rbxassetid://" .. 10921262864
    Client.Character.Animate.swim.Swim.AnimationId = "rbxassetid://" .. 10921264784
    Client.Character.Animate.swimidle.SwimIdle.AnimationId = "rbxassetid://" .. 10921265698
    Client["Character"]:FindFirstChild("Animate").Disabled = false
    Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    Humanoid:ChangeState("Jumping")
end

