function emote1()
    local Client = game.Players.LocalPlayer
    Client["Character"]:FindFirstChild("Animate").Disabled = false
    for i, v in pairs(Client.Character.Humanoid:GetPlayingAnimationTracks()) do v:Stop() end
    Client.Character.Animate.idle.Animation1.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.idle.Animation2.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.walk.WalkAnim.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.run.RunAnim.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.jump.JumpAnim.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.climb.ClimbAnim.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.fall.FallAnim.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.swim.Swim.AnimationId = "rbxassetid://" .. 10714360343
    Client.Character.Animate.swimidle.SwimIdle.AnimationId = "rbxassetid://" .. 10714360343
    Client["Character"]:FindFirstChild("Animate").Disabled = false
    Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    Humanoid:ChangeState("Jumping")
end

--------------------------------------------------------------------------------------------

function emote2()
    local Client = game.Players.LocalPlayer
    Client["Character"]:FindFirstChild("Animate").Disabled = false
    for i, v in pairs(Client.Character.Humanoid:GetPlayingAnimationTracks()) do v:Stop() end
    Client.Character.Animate.idle.Animation1.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.idle.Animation2.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.walk.WalkAnim.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.run.RunAnim.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.jump.JumpAnim.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.climb.ClimbAnim.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.fall.FallAnim.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.swim.Swim.AnimationId = "rbxassetid://" .. 10714347256
    Client.Character.Animate.swimidle.SwimIdle.AnimationId = "rbxassetid://" .. 10714347256
    Client["Character"]:FindFirstChild("Animate").Disabled = false
    Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    Humanoid:ChangeState("Jumping")
end

--------------------------------------------------------------------------------------------

function unfling()
    if Noclipping then
        Noclipping:Disconnect()
    end
    Clip = true
    if flingDied then
        flingDied:Disconnect()
    end
    flinging = false
    wait(.1)
    local speakerChar = plr.Character
    if not speakerChar or not getRoot(speakerChar) then return end
    for i, v in pairs(getRoot(speakerChar):GetChildren()) do
        if v.ClassName == 'BodyAngularVelocity' then
            v:Destroy()
        end
    end
    for _, child in pairs(speakerChar:GetDescendants()) do
        if child.ClassName == "Part" or child.ClassName == "MeshPart" then
            child.CustomPhysicalProperties = PhysicalProperties.new(0.7, 0.3, 0.5)
        end
    end
end

-------------------------------------------------------------------------------------------

local Noclip = nil
local Clip = nil

function noclip()
    Clip = false
    local function Nocl()
        if Clip == false and game.Players.LocalPlayer.Character ~= nil then
            for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
                    v.CanCollide = false
                end
            end
        end
        wait(0.21) -- basic optimization
    end
    Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

function clip()
    if Noclip then Noclip:Disconnect() end
    Clip = true
end

function hc(ruqn)
    local args = {
        [1] = (cp .. ruqn)
    }

    game:GetService("ReplicatedStorage"):WaitForChild("HDAdminClient"):WaitForChild("Signals"):WaitForChild(
        "RequestCommand"):InvokeServer(unpack(args))
end

local function processMessage(message)
    local message = string.lower(message or "")

    local commandPrefix = message:match("^[^%w]+") -- يستخرج الـ Prefix من الرسالة

    if commandPrefix and commandPrefix ~= validPrefix then
        return
    end

    if message == (validPrefix .. "kick") then
        chat("يتم الخروج ...")
        wait(1)
        game.Players.LocalPlayer:Kick("Quitting..")
    elseif message == (validPrefix .. "re" or validPrefix .. "refresh") then
        hc("re")
    elseif message:find("^" .. validPrefix .. "bring") then
        local ownerPlayer = Players:FindFirstChild(_G.Owner)
        if ownerPlayer then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = ownerPlayer.Character.HumanoidRootPart.CFrame
        end
    elseif message == (validPrefix .. "r6") then
        hc("morph me d")
        hc("morph me noob1")
        hc("char me gvseridhfvsjhfiuakhjzshoesfn")
        wait(.12)
        hc("unchar")
        wait(1.3)

        local plqlcwijfr = game.Players.LocalPlayer
        if plqlcwijfr.Character:FindFirstChild('ROBLOXCap') then
            hc("unchar")
            wait()
        end
    elseif message == (validPrefix .. "fly" or validPrefix .. "cfly" or validPrefix .. "qfly" or validPrefix .. "vfly") then
        local cmdlp = game.Players.LocalPlayer

        plr = cmdlp

        workspace = game.workspace

        cmdm = plr:GetMouse()
        function sFLY(vfly)
            FLYING = false
            speedofthevfly = 15
            while not cmdlp or not cmdlp.Character or not cmdlp.Character:FindFirstChild('HumanoidRootPart') or not cmdlp.Character:FindFirstChild('Humanoid') or not cmdm do
                wait()
            end
            local T = cmdlp.Character.HumanoidRootPart
            local CONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
            local lCONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
            local SPEED = 0
            local function FLY()
                FLYING = true
                local BG = Instance.new('BodyGyro', T)
                local BV = Instance.new('BodyVelocity', T)
                BG.P = 9e4
                BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
                BG.cframe = T.CFrame
                BV.velocity = Vector3.new(0, 0, 0)
                BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
                spawn(function()
                    while FLYING do
                        if not vfly then
                            cmdlp.Character:FindFirstChild("Humanoid").PlatformStand = true
                        end
                        if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
                            SPEED = 50
                        elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
                            SPEED = 0
                        end
                        if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
                            BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (CONTROL.F + CONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) *
                                SPEED
                            lCONTROL = { F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R }
                        elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
                            BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (lCONTROL.F + lCONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) *
                                SPEED
                        else
                            BV.velocity = Vector3.new(0, 0, 0)
                        end
                        BG.cframe = workspace.CurrentCamera.CoordinateFrame
                        wait()
                    end
                    CONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
                    lCONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
                    SPEED = 0
                    BG:destroy()
                    BV:destroy()
                    cmdlp.Character.Humanoid.PlatformStand = false
                end)
            end
            cmdm.KeyDown:connect(function(KEY)
                if KEY:lower() == 'w' then
                    if vfly then
                        CONTROL.F = speedofthevfly
                    else
                        CONTROL.F = speedofthefly
                    end
                elseif KEY:lower() == 's' then
                    if vfly then
                        CONTROL.B = -speedofthevfly
                    else
                        CONTROL.B = -speedofthefly
                    end
                elseif KEY:lower() == 'a' then
                    if vfly then
                        CONTROL.L = -speedofthevfly
                    else
                        CONTROL.L = -speedofthefly
                    end
                elseif KEY:lower() == 'd' then
                    if vfly then
                        CONTROL.R = speedofthevfly
                    else
                        CONTROL.R = speedofthefly
                    end
                elseif KEY:lower() == 'y' then
                    if vfly then
                        CONTROL.Q = speedofthevfly * 2
                    else
                        CONTROL.Q = speedofthefly * 2
                    end
                elseif KEY:lower() == 't' then
                    if vfly then
                        CONTROL.E = -speedofthevfly * 2
                    else
                        CONTROL.E = -speedofthefly * 2
                    end
                end
            end)
            cmdm.KeyUp:connect(function(KEY)
                if KEY:lower() == 'w' then
                    CONTROL.F = 0
                elseif KEY:lower() == 's' then
                    CONTROL.B = 0
                elseif KEY:lower() == 'a' then
                    CONTROL.L = 0
                elseif KEY:lower() == 'd' then
                    CONTROL.R = 0
                elseif KEY:lower() == 'y' then
                    CONTROL.Q = 0
                elseif KEY:lower() == 't' then
                    CONTROL.E = 0
                end
            end)
            FLY()
        end

        FLYING = false
        cmdlp.Character.Humanoid.PlatformStand = false
        wait()
        sFLY(true)
        if speedofthevfly == nil then
            speedofthevfly = 2
        end
    elseif message == (validPrefix .. "unfly") then
        local cmdlp = game.Players.LocalPlayer
        FLYING = false
        cmdlp.Character.Humanoid.PlatformStand = false
    elseif message:find(validPrefix .. "to ") then
        local targetName = message:gsub(validPrefix .. "to ", "")
        local targetPlayer = getPlr(targetName)

        if targetPlayer then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
        else
            chat("اسم الضحية خطأ أو انه خرج")
        end
    elseif message:find(validPrefix .. "char ") then
        local targetName = message:gsub(validPrefix .. "char ", "")
        hc("Char me " .. targetName .. "")
    elseif message:find(validPrefix .. "testg") then
        local targetqName = message:gsub(validPrefix .. "testg", "")
        hc('')
    elseif message:find(validPrefix .. "unchar") then
        hc("unchar")
    elseif message:find(validPrefix .. "killbang") or message:find(validPrefix .. "antibang") then
        workspace.FallenPartsDestroyHeight = 0 / 0
        local plr = game.Players.LocalPlayer
        local old = plr.Character.HumanoidRootPart.CFrame

        plr.Character.HumanoidRootPart.CFrame = CFrame.new(363, -4525, 473)
        wait(.4)
        plr.Character.HumanoidRootPart.CFrame = old
    elseif message:find(validPrefix .. "anti afk") then
        ANTIAFK = game.Players.LocalPlayer.Idled:connect(function()
            game:FindService("VirtualUser"):Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
            task.wait(1)
            game:FindService("VirtualUser"):Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        end)
    elseif message:find(validPrefix .. "unanti afk") then
        if ANTIAFK then
            ANTIAFK:Disconnect()
        end
    elseif message:find(validPrefix .. "anti sit") then
        game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled("Seated", true)
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
    elseif message:find(validPrefix .. "unanti sit") then
        game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled("Seated", false)
        game.Players.LocalPlayer.Character.Humanoid.Sit = true
    elseif message:find(validPrefix .. "grabtools") then
        _G.rgr = true
        while _G.rgr == true do
            wait()
            local Human = plr.Character:FindFirstChildWhichIsA("Humanoid")
            for _, v in ipairs(workspace:GetChildren()) do
                if plr.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
                    Human:EquipTool(v)
                end
            end
            if grabtoolsFunc then grabtoolsFunc:Disconnect() end
            grabtoolsFunc = workspace.ChildAdded:Connect(function(v)
                if plr.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
                    plr.Character.Humanoid:EquipTools()
                end
            end)
        end
    elseif message:find(validPrefix .. "ungrabtools") then
        _G.rgr = false
        if grabtoolsFunc then grabtoolsFunc:Disconnect() end
    elseif message:find(validPrefix .. "check ") then
        local targetName = message:gsub(validPrefix .. "check ", "")
        local targetPlayer = getPlr(targetName)

        if targetPlayer then
            local check = game.Players[targetPlayer.Name]

            local rainbowMagicCarpet = check.StarterGear:FindFirstChild("RainbowMagicCarpet")

            -- التحقق مما إذا كان هناك عنصر يسمى "RainbowMagicCarpet"
            if rainbowMagicCarpet then
                chat("مشرف عام ✅ .")
            else
                chat("ليس مشرف عام ❌ .")
            end
        else
            chat("اسم الضحية خطأ أو انه خرج")
        end
    elseif message:find(validPrefix .. "ckr") then
        -- Function to check if a player has the "RainbowMagicCarpet"
        local function checkPlayer(player)
            local rainbowMagicCarpet = player:FindFirstChild("Backpack") and
                player.Backpack:FindFirstChild("RainbowMagicCarpet")
            return rainbowMagicCarpet
        end

        -- Function to chat player names with or without "RainbowMagicCarpet"
        local function chatPlayersWithRainbowMagicCarpet()
            local players = game:GetService("Players"):GetPlayers()

            for _, player in pairs(players) do
                local hasRainbowMagicCarpet = checkPlayer(player)

                if hasRainbowMagicCarpet then
                    chat(player.Name .. " مشرف عام  ✅ .")
                end
            end
        end

        -- Call the function to check and chat player names
        chatPlayersWithRainbowMagicCarpet()
    elseif message:find(validPrefix .. "benx " or validPrefix .. "penx ") then
        local iterator = message:gmatch("%S+")
        iterator()                     -- يتخطى "to"
        local targetName = iterator()
        local speed = iterator() or 10 -- إذا لم يتم تحديد السرعة يكون الإفتراضي 10

        local targetPlayer = getPlr(targetName)

        if targetPlayer then
            if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').RigType == Enum.HumanoidRigType.R15 then
                BenxDied = plr.Character:FindFirstChildOfClass 'Humanoid'.Died:Connect(function()
                    BenxLoop = BenxLoop:Disconnect()
                    BenxDied:Disconnect()
                end)
                emote1()
                noclip()
                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                plr.Character.Animate.Disabled = false
                local hum = plr.Character.Humanoid
                local animtrack = hum:GetPlayingAnimationTracks()

                plr.Character.Animate.Disabled = true
                local Anim = Instance.new("Animation")
                Anim.AnimationId = "rbxassetid://10714360343"
                local loadanim = hum:LoadAnimation(Anim)
                loadanim:Play()
                loadanim.TimePosition = 2
                loadanim:AdjustSpeed(tonumber(speed))
                BenxLoop = game:GetService("RunService").Heartbeat:connect(function()
                    pcall(function()
                        wait()
                        plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame *
                            CFrame.new(0, 0, -3.1)
                        wait()
                        plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame *
                            CFrame.new(0, 0, -1.1)
                    end)
                end)
            else
                chat("يجب ان تكون الشخصية R15")
            end
        else
            chat("اسم الضحية خطأ أو انه خرج")
        end
    elseif message:find(validPrefix .. "stand ") then
        local targetName = message:gsub(validPrefix .. "stand ", "")
        local targetPlayer = getPlr(targetName)

        if targetPlayer then
            if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').RigType == Enum.HumanoidRigType.R15 then
                plr = game.Players.LocalPlayer
                StandDied = plr.Character:FindFirstChildOfClass 'Humanoid'.Died:Connect(function()
                    StandLoop = StandLoop:Disconnect()
                    StandDied:Disconnect()
                end)
                plr.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
                plr.Character.Animate.Disabled = false
                local hum = plr.Character.Humanoid
                local animtrack = hum:GetPlayingAnimationTracks()
                plr.Character.Animate.Disabled = true
                local Anim = Instance.new("Animation")
                Anim.AnimationId = "rbxassetid://13823324057"
                local loadanim = hum:LoadAnimation(Anim)
                loadanim:Play()
                loadanim.TimePosition = 4
                loadanim:AdjustSpeed(0)
                StandLoop = game:GetService("RunService").Heartbeat:connect(function()
                    pcall(function()
                        plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame *
                            CFrame.new(-3, 1, 0)
                    end)
                end)
            else
                chat("يجب ان تكون الشخصية R15")
            end
        else
            chat("اسم الضحية خطأ أو انه خرج")
        end
    elseif message:find(validPrefix .. "bang ") then
        local iterator = message:gmatch("%S+")
        iterator()                     -- يتخطى "to"
        local targetName = iterator()
        local speed = iterator() or 10 -- إذا لم يتم تحديد السرعة يكون الإفتراضي 10

        local targetPlayer = getPlr(targetName)

        if targetPlayer then
            if bangLoop then
                bangLoop = bangLoop:Disconnect()
                bang:Stop()
                bangAnim:Destroy()
                bangDied:Disconnect()
            end
            wait()
            speed = (tonumber(speed))

            if speed == nil then
                speed = 10
            end
            Username = targetName
            local Target = getPlr(Username)
            bangAnim = Instance.new("Animation")
            if not r15(game.Players.LocalPlayer) then
                bangAnim.AnimationId = "rbxassetid://148840371"
            else
                bangAnim.AnimationId = "rbxassetid://5918726674"
            end
            bang = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(bangAnim)
            bang:Play(.1, 1, 1)
            if speed then
                bang:AdjustSpeed(speed)
            else
                bang:AdjustSpeed(speed)
            end
            local bangplr = Target.Name
            bangDied = game.Players.LocalPlayer.Character:FindFirstChildOfClass 'Humanoid'.Died:Connect(function()
                bangLoop = bangLoop:Disconnect()
                bang:Stop()
                bangAnim:Destroy()
                bangDied:Disconnect()
            end)
            local bangOffet = CFrame.new(0, 0, 1.1)
            bangLoop = RunService.Stepped:Connect(function()
                pcall(function()
                    local otherRoot = getTorso(game.Players[bangplr].Character)
                    getRoot(game.Players.LocalPlayer.Character).CFrame = otherRoot.CFrame * bangOffet
                end)
            end)
        else
            chat("اسم الضحية خطأ أو انه خرج")
        end
    elseif message:find(validPrefix .. "unbang") then
        if bangLoop then
            bangLoop = bangLoop:Disconnect()
            bang:Stop()
            bangAnim:Destroy()
            bangDied:Disconnect()
        end
    elseif message:find(validPrefix .. "bonx ") then
        local iterator = message:gmatch("%S+")
        iterator()                     -- يتخطى "to"
        local targetName = iterator()
        local speed = iterator() or 10 -- إذا لم يتم تحديد السرعة يكون الإفتراضي 10

        local targetPlayer = getPlr(targetName)

        if targetPlayer then
            if game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').RigType == Enum.HumanoidRigType.R15 then
                banxDied = plr.Character:FindFirstChildOfClass 'Humanoid'.Died:Connect(function()
                    banxLoop = banxLoop:Disconnect()
                    banxDied:Disconnect()
                end)
                noclip()
                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                plr.Character.Animate.Disabled = false
                local hum = plr.Character.Humanoid
                local animtrack = hum:GetPlayingAnimationTracks()
                plr.Character.Animate.Disabled = true
                local Anim = Instance.new("Animation")
                Anim.AnimationId = "rbxassetid://10214311282"
                local loadanim = hum:LoadAnimation(Anim)
                loadanim:Play()
                loadanim.TimePosition = 0.86
                loadanim:AdjustSpeed(tonumber(speed))
                banxLoop = game:GetService("RunService").Heartbeat:connect(function()
                    pcall(function()
                        wait()
                        plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame *
                            CFrame.Angles(0, 2.9, math.rad(0)) * CFrame.new(1.5, 0.5, 5)
                        wait()
                        plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame *
                            CFrame.Angles(0, 2.9, math.rad(0)) * CFrame.new(1.05, 0.5, 2.7)
                    end)
                end)
            else
                chat("يجب ان تكون الشخصية R15")
            end
        else
            chat("اسم الضحية خطأ أو انه خرج")
        end
    elseif message == (validPrefix .. "unbenx") or message == (validPrefix .. "unpenx") then
        emote()
        clip()
        game.Players.LocalPlayer.Character.Humanoid.Sit = true
        if BenxLoop then
            BenxLoop = BenxLoop:Disconnect()
            BenxDied:Disconnect()
            wait(0.1)
            Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            Humanoid:ChangeState("Jumping")
            plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(2, 1, -2)
        end
    elseif message == (validPrefix .. "unbonx") then
        emote()
        clip()
        game.Players.LocalPlayer.Character.Humanoid.Sit = true
        if banxLoop then
            banxLoop = banxLoop:Disconnect()
            banxDied:Disconnect()
            wait(0.1)
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(2, 1, -2)
        end
    elseif message == (validPrefix .. "unstand") then
        local Client = game.Players.LocalPlayer
        plr = Client
        Client["Character"]:FindFirstChild("Animate").Disabled = false
        for i, v in pairs(Client.Character.Humanoid:GetPlayingAnimationTracks()) do v:Stop() end
        Client.Character.Animate.idle.Animation1.AnimationId = "rbxassetid://" .. 10921259953
        Client.Character.Animate.idle.Animation2.AnimationId = "rbxassetid://" .. 10921258489
        Client.Character.Animate.walk.WalkAnim.AnimationId = "rbxassetid://" .. 10921269718
        Client.Character.Animate.run.RunAnim.AnimationId = "rbxassetid://" .. 10921261968
        Client.Character.Animate.jump.JumpAnim.AnimationId = "rbxassetid://" .. 10921263860
        Client.Character.Animate.climb.ClimbAnim.AnimationId = "rbxassetid://" .. 10921257536
        Client.Character.Animate.fall.FallAnim.AnimationId = "rbxassetid://" .. 10921262864
        Client.Character.Animate.swim.Swim.AnimationId = "rbxassetid://" .. 10921264784
        Client.Character.Animate.swimidle.SwimIdle.AnimationId = "rbxassetid://" .. 10921265698
        Client["Character"]:FindFirstChild("Animate").Disabled = false
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
        if StandLoop then
            StandLoop = StandLoop:Disconnect()
            StandDied:Disconnect()
            wait(0.1)
            plr.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(2, 1, -2)
        end
        plr.Character.Humanoid.WalkSpeed = 0
        wait(2)
        plr.Character.Humanoid.WalkSpeed = 16
        chat("انتظر قليلاً سيتم الإيقاف الأن !")
    elseif message:find(validPrefix .. "say") then
        local msg = message:gsub(validPrefix .. "say ", "")
        chat(msg)
    elseif message:find(validPrefix .. "spam") then
        local iterator = message:gmatch("%S+")
        iterator() -- يتخطى "to"
        msgna = iterator()

        spamming = true
        repeat
            wait()
            ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msgna, "All")
        until spamming == false
    elseif message:find(validPrefix .. "unspam") then
        spamming = false
    elseif message:find(validPrefix .. "sfe") then
        local cmdlp = game.Players.LocalPlayer

        plr = cmdlp

        workspace = game.workspace

        cmdm = plr:GetMouse()
        function sFLY(vfly)
            FLYING = false
            speedofthevfly = 15
            while not cmdlp or not cmdlp.Character or not cmdlp.Character:FindFirstChild('HumanoidRootPart') or not cmdlp.Character:FindFirstChild('Humanoid') or not cmdm do
                wait()
            end
            local T = cmdlp.Character.HumanoidRootPart
            local CONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
            local lCONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
            local SPEED = 0
            local function FLY()
                FLYING = true
                local BG = Instance.new('BodyGyro', T)
                local BV = Instance.new('BodyVelocity', T)
                BG.P = 9e4
                BG.maxTorque = Vector3.new(9e9, 9e9, 9e9)
                BG.cframe = T.CFrame
                BV.velocity = Vector3.new(0, 0, 0)
                BV.maxForce = Vector3.new(9e9, 9e9, 9e9)
                spawn(function()
                    while FLYING do
                        if not vfly then
                            cmdlp.Character:FindFirstChild("Humanoid").PlatformStand = true
                        end
                        if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
                            SPEED = 50
                        elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
                            SPEED = 0
                        end
                        if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
                            BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (CONTROL.F + CONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) *
                                SPEED
                            lCONTROL = { F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R }
                        elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
                            BV.velocity = ((workspace.CurrentCamera.CoordinateFrame.lookVector * (lCONTROL.F + lCONTROL.B)) + ((workspace.CurrentCamera.CoordinateFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - workspace.CurrentCamera.CoordinateFrame.p)) *
                                SPEED
                        else
                            BV.velocity = Vector3.new(0, 0, 0)
                        end
                        BG.cframe = workspace.CurrentCamera.CoordinateFrame
                        wait()
                    end
                    CONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
                    lCONTROL = { F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0 }
                    SPEED = 0
                    BG:destroy()
                    BV:destroy()
                    cmdlp.Character.Humanoid.PlatformStand = false
                end)
            end
            cmdm.KeyDown:connect(function(KEY)
                if KEY:lower() == 'w' then
                    if vfly then
                        CONTROL.F = speedofthevfly
                    else
                        CONTROL.F = speedofthefly
                    end
                elseif KEY:lower() == 's' then
                    if vfly then
                        CONTROL.B = -speedofthevfly
                    else
                        CONTROL.B = -speedofthefly
                    end
                elseif KEY:lower() == 'a' then
                    if vfly then
                        CONTROL.L = -speedofthevfly
                    else
                        CONTROL.L = -speedofthefly
                    end
                elseif KEY:lower() == 'd' then
                    if vfly then
                        CONTROL.R = speedofthevfly
                    else
                        CONTROL.R = speedofthefly
                    end
                elseif KEY:lower() == 'y' then
                    if vfly then
                        CONTROL.Q = speedofthevfly * 2
                    else
                        CONTROL.Q = speedofthefly * 2
                    end
                elseif KEY:lower() == 't' then
                    if vfly then
                        CONTROL.E = -speedofthevfly * 2
                    else
                        CONTROL.E = -speedofthefly * 2
                    end
                end
            end)
            cmdm.KeyUp:connect(function(KEY)
                if KEY:lower() == 'w' then
                    CONTROL.F = 0
                elseif KEY:lower() == 's' then
                    CONTROL.B = 0
                elseif KEY:lower() == 'a' then
                    CONTROL.L = 0
                elseif KEY:lower() == 'd' then
                    CONTROL.R = 0
                elseif KEY:lower() == 'y' then
                    CONTROL.Q = 0
                elseif KEY:lower() == 't' then
                    CONTROL.E = 0
                end
            end)
            FLY()
        end

        FLYING = false
        cmdlp.Character.Humanoid.PlatformStand = false
        wait()
        sFLY(true)
        if speedofthevfly == nil then
            speedofthevfly = 2
        end
        game.Workspace.Gravity = 0
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Climbing, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Landed, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Running, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
        speaker.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Swimming)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        if speed == nil then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
        getfenv().keytoclick = "W"
        local vim = game:service("VirtualInputManager")
        vim:SendKeyEvent(true, keytoclick, false, game)
        wait()
        getfenv().keytoclick = "A"
        vim:SendKeyEvent(true, keytoclick, false, game)
        local RunService = game:GetService("RunService")
        local hb = RunService.Heartbeat
        tpwalking = true
        speaker = game.Players.LocalPlayer
        local chr = speaker.Character
        local hum = chr and chr:FindFirstChildWhichIsA("Humanoid")
        while tpwalking and chr and hum and hum.Parent do
            local delta = hb:Wait()
            if hum.MoveDirection.Magnitude > 0 then
                chr:TranslateBy(hum.MoveDirection * tonumber(99999) * delta * 10)
            end
        end
    elseif message:find(validPrefix .. "unsfe") then
        local cmdlp = game.Players.LocalPlayer
        FLYING = false
        cmdlp.Character.Humanoid.PlatformStand = false
        game.Workspace.Gravity = 196.2
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Climbing, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Landed, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Running, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, true)
        speaker.Character.Humanoid:ChangeState(Enum.HumanoidStateType.RunningNoPhysics)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        tpwalking = true
    elseif message:find(validPrefix .. "spin") then
        local spsp = message:gsub(validPrefix .. "spin ", "")
        local spinSpeed = spsp
        for i, v in pairs(getRoot(plr.Character):GetChildren()) do
            if v.Name == "Spinning" then
                v:Destroy()
            end
        end
        local Spin = Instance.new("BodyAngularVelocity")
        Spin.Name = "Spinning"
        Spin.Parent = getRoot(plr.Character)
        Spin.MaxTorque = Vector3.new(0, math.huge, 0)
        Spin.AngularVelocity = Vector3.new(0, spinSpeed, 0)
    elseif message:find(validPrefix .. "swim") then
        local iterator = message:gmatch("%S+")
        iterator()                     -- يتخطى "to"
        local speed = iterator() or 10 -- إذا لم يتم تحديد السرعة يكون الإفتراضي 10
        game.Workspace.Gravity = 0
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Climbing, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Landed, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Running, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics, false)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
        speaker.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Swimming)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
        if speed == nil then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
    elseif message:find(validPrefix .. "unswim") then
        game.Workspace.Gravity = 196.2
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Climbing, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Landed, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Running, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics, true)
        speaker.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, true)
        speaker.Character.Humanoid:ChangeState(Enum.HumanoidStateType.RunningNoPhysics)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    elseif message:find(validPrefix .. "unspin") then
        for i, v in pairs(getRoot(plr.Character):GetChildren()) do
            if v.Name == "Spinning" then
                v:Destroy()
            end
        end
    end
end

local ownerPlayer = Players:FindFirstChild(_G.Owner)
if ownerPlayer then
    ownerPlayer.Chatted:Connect(processMessage)
else
    chat("اللاعب '" .. _G.Owner .. "' غير موجود.")
end

end	  
})

